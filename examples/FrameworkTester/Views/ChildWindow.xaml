                    Text="{Binding Session.Handle, Converter={StaticResource IntegerToHexStringConveter}}" />
                <TextBlock
                    Grid.Row="8"
                    Grid.Column="0"
                    Style="{StaticResource LabelTextBlock}"
                    Text="UnitId: " />
                <TextBlock
                    Grid.Row="8"
                    Grid.Column="2"
                    Style="{StaticResource ValueTextBlock}"
                    Text="{Binding UnitId}" />
                <TextBlock
                    Grid.Row="10"
                    Grid.Column="0"
                    Style="{StaticResource LabelTextBlock}"
                    Text="UserData: " />
                <TextBlock
                    Grid.Row="10"
                    Grid.Column="2"
                    Style="{StaticResource ValueTextBlock}"
                    Text="{Binding UserData, Converter={StaticResource IntegerToHexStringConveter}}" />
                <TextBlock
                    Grid.Row="12"
                    Grid.Column="0"
                    Style="{StaticResource LabelTextBlock}"
                    Text="TimeStamp: " />
                <TextBlock
                    Grid.Row="12"
                    Grid.Column="2"
                    Style="{StaticResource ValueTextBlock}"
                    Text="{Binding Path=TimeStamp, StringFormat=yyyy/MM/dd hh:mm:ss.fff}" />
            </Grid>
        </DataTemplate>

        <selectors:ChildWindowTemplateSlector x:Key="ChildWindowTemplateSlector"
                                              AsyncOpenFramework="{StaticResource AsyncOpenFrameworkTemplate}"
                                              AsyncOpenSession="{StaticResource AsyncOpenSessionTemplate}" />

        <DataTemplate x:Key="ChildWindowLogTemplateSlectorCaptureSample" DataType="viewModels:AsyncResultCaptureSampleDummy">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="5" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="5" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="5" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="5" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="5" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="5" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="5" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="5" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <TextBlock
                    Grid.Row="0"
                    Grid.Column="2"
                    Margin="0,0,256,0"
                    Style="{StaticResource LabelTextBlock}"
                    Text="Captured Image: " />
                <Viewbox
                    Grid.Row="0"
                    Grid.RowSpan="16"
                    Grid.Column="2"
                    HorizontalAlignment="Right"
                    VerticalAlignment="Top"
                    Style="{StaticResource CaptureSampleViewBox}">
                    <Image Source="{Binding CaptureImage}" Stretch="UniformToFill" />
                </Viewbox>
                <TextBlock
                    Grid.Row="0"
                    Grid.Column="0"
                    Style="{StaticResource LabelTextBlock}"
                    Text="SampleSize: " />
                <TextBlock
                    Grid.Row="0"
                    Grid.Column="2"
                    Style="{StaticResource ValueTextBlock}"
                    Text="{Binding SampleSize}" />
                <TextBlock
                    Grid.Row="2"
                    Grid.Column="0"
                    Style="{StaticResource LabelTextBlock}"
                    Text="RejectDetail: " />
                <TextBlock
                    Grid.Row="2"
                    Grid.Column="2"
                    Style="{StaticResource ValueTextBlock}"
                    Text="{Binding RejectDetail}" />
                <TextBlock
                    Grid.Row="4"
                    Grid.Column="0"
                    Style="{StaticResource LabelTextBlock}"
                    Text="Width: " />
                <TextBlock
                    Grid.Row="4"
                    Grid.Column="2"
                    Style="{StaticResource ValueTextBlock}"
                    Text="{Binding Sample.Width}" />
                <TextBlock
                    Grid.Row="6"
                    Grid.Column="0"
                    Style="{StaticResource LabelTextBlock}"
                    Text="Height: " />
                <TextBlock
                    Grid.Row="6"
                    Grid.Column="2"
                    Style="{StaticResource ValueTextBlock}"
                    Text="{Binding Sample.Height}" />
                <TextBlock
                    Grid.Row="8"
                    Grid.Column="0"
                    Style="{StaticResource LabelTextBlock}"
                    Text="HorizontalResolution: " />
                <TextBlock
                    Grid.Row="8"
                    Grid.Column="2"
                    Style="{StaticResource ValueTextBlock}"
                    Text="{Binding Sample.HorizontalImageResolution}" />
                <TextBlock
                    Grid.Row="10"
                    Grid.Column="0"
                    Style="{StaticResource LabelTextBlock}"
                    Text="VerticalResolution: " />
                <TextBlock
                    Grid.Row="10"
                    Grid.Column="2"
                    Style="{StaticResource ValueTextBlock}"
                    Text="{Binding Sample.VerticalImageResolution}" />
                <TextBlock
                    Grid.Row="12"
                    Grid.Column="0"
                    Style="{StaticResource LabelTextBlock}"
                    Text="ScanHorizontalResolution: " />
                <TextBlock
                    Grid.Row="12"
                    Grid.Column="2"
                    Style="{StaticResource ValueTextBlock}"
                    Text="{Binding Sample.HorizontalScanResolution}" />
                <TextBlock
                    Grid.Row="14"
                    Grid.Column="0"
                    Style="{StaticResource LabelTextBlock}"
                    Text="ScanVerticalResolution: " />
                <TextBlock
                    Grid.Row="14"
                    Grid.Column="2"
                    Style="{StaticResource ValueTextBlock}"
                    Text="{Binding Sample.VerticalScanResolution}" />
            </Grid>
        </DataTemplate>
        <DataTemplate x:Key="ChildWindowLogTemplateSlectorDeleteTemplate" DataType="winBiometricDotNet:AsyncResultDeleteTemplate">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="5" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="5" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <TextBlock
                    Grid.Row="0"
                    Grid.Column="0"
                    VerticalAlignment="Top"
                    Style="{StaticResource LabelTextBlock}"
                    Text="Identity: " />
                <Grid
                    Grid.Row="0"
                    Grid.Column="2"
                    DataContext="{Binding Identity}">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="5" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="5" />
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="5" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <TextBlock
                        Grid.Row="0"
                        Grid.Column="0"
                        Style="{StaticResource LabelTextBlock}"
                        Text="Type: " />
                    <TextBlock
                        Grid.Row="0"
                        Grid.Column="2"
                        Style="{StaticResource ValueTextBlock}"
                        Text="{Binding Type}" />
                    <TextBlock
                        Grid.Row="2"
                        Grid.Column="0"
                        Style="{StaticResource LabelTextBlock}"
                        Text="TemplateGuid: " />
                    <TextBlock
                        Grid.Row="2"
                        Grid.Column="2"
                        Style="{StaticResource ValueTextBlock}"
                        Text="{Binding TemplateGuid}" />
                    <TextBlock
                        Grid.Row="4"
                        Grid.Column="0"
                        Style="{StaticResource LabelTextBlock}"
                        Text="Sid: " />
                    <TextBlock
                        Grid.Row="4"
                        Grid.Column="2"
                        Style="{StaticResource ValueTextBlock}"
                        Text="{Binding Sid}" />
                </Grid>
                <TextBlock
                    Grid.Row="2"
                    Grid.Column="0"
                    Style="{StaticResource LabelTextBlock}"
                    Text="FingerPosition: " />
                <TextBlock
                    Grid.Row="2"
                    Grid.Column="2"
                    Style="{StaticResource ValueTextBlock}"
                    Text="{Binding FingerPosition}" />
            </Grid>
        </DataTemplate>
        <DataTemplate x:Key="ChildWindowLogTemplateSlectorEnrollBegin" DataType="winBiometricDotNet:AsyncResultEnrollBegin">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="5" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <TextBlock
                    Grid.Row="0"
                    Grid.Column="0"
                    Style="{StaticResource LabelTextBlock}"
                    Text="FingerPosition: " />
                <TextBlock
                    Grid.Row="0"
                    Grid.Column="2"
                    Style="{StaticResource ValueTextBlock}"
                    Text="{Binding FingerPosition}" />
            </Grid>
        </DataTemplate>
        <DataTemplate x:Key="ChildWindowLogTemplateSlectorEnrollCapture" DataType="winBiometricDotNet:AsyncResultEnrollCapture">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
