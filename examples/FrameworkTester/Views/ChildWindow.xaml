<Window
    x:Class="FrameworkTester.Views.ChildWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:controls="clr-namespace:FrameworkTester.Controls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:designTimes="clr-namespace:FrameworkTester.DesignTimes"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:selectors="clr-namespace:FrameworkTester.Selectors"
    xmlns:viewModels="clr-namespace:FrameworkTester.ViewModels"
    xmlns:winBiometricDotNet="clr-namespace:WinBiometricDotNet;assembly=WinBiometricDotNet"
    x:Name="_Window"
    Title="{Binding Handle, Converter={StaticResource IntegerToHexStringConveter}}"
    Width="600"
    Height="650"
    d:DataContext="{d:DesignInstance {x:Type designTimes:ChildWindowViewModel}}"
    d:DesignHeight="650"
    d:DesignWidth="600"
    ShowInTaskbar="False"
    mc:Ignorable="d">

    <Window.Resources>
        <DataTemplate x:Key="AsyncOpenFrameworkTemplate" DataType="winBiometricDotNet:AsyncResult">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="5" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="5" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="5" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="5" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="5" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="5" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="5" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <TextBlock
                    Grid.Row="0"
                    Grid.Column="0"
                    Style="{StaticResource LabelTextBlock}"
                    Text="ApiStatus: " />
                <TextBlock
                    Grid.Row="0"
                    Grid.Column="2"
                    Style="{StaticResource ValueTextBlock}"
                    Text="{Binding ApiStatus}" />
                <TextBlock
                    Grid.Row="2"
                    Grid.Column="0"
                    Style="{StaticResource LabelTextBlock}"
                    Text="OperationType: " />
                <TextBlock
                    Grid.Row="2"
                    Grid.Column="2"
                    Style="{StaticResource ValueTextBlock}"
                    Text="{Binding OperationType}" />
                <TextBlock
                    Grid.Row="4"
                    Grid.Column="0"
                    Style="{StaticResource LabelTextBlock}"
                    Text="SequenceNumber: " />
                <TextBlock
                    Grid.Row="4"
                    Grid.Column="2"
                    Style="{StaticResource ValueTextBlock}"
                    Text="{Binding SequenceNumber}" />
                <TextBlock
                    Grid.Row="6"
                    Grid.Column="0"
                    Style="{StaticResource LabelTextBlock}"
                    Text="Framework Handle: " />
                <TextBlock
                    Grid.Row="6"
                    Grid.Column="2"
                    Style="{StaticResource ValueTextBlock}"
                    Text="{Binding Framework.Handle, Converter={StaticResource IntegerToHexStringConveter}}" />
                <TextBlock
                    Grid.Row="8"
                    Grid.Column="0"
                    Style="{StaticResource LabelTextBlock}"
                    Text="UnitId: " />
                <TextBlock
                    Grid.Row="8"
                    Grid.Column="2"
                    Style="{StaticResource ValueTextBlock}"
                    Text="{Binding UnitId}" />
                <TextBlock
                    Grid.Row="10"
                    Grid.Column="0"
                    Style="{StaticResource LabelTextBlock}"
                    Text="UserData: " />
                <TextBlock
                    Grid.Row="10"
                    Grid.Column="2"
                    Style="{StaticResource ValueTextBlock}"
                    Text="{Binding UserData, Converter={StaticResource IntegerToHexStringConveter}}" />
                <TextBlock
                    Grid.Row="12"
                    Grid.Column="0"
                    Style="{StaticResource LabelTextBlock}"
                    Text="TimeStamp: " />
                <TextBlock
                    Grid.Row="12"
                    Grid.Column="2"
                    Style="{StaticResource ValueTextBlock}"
                    Text="{Binding Path=TimeStamp, StringFormat=yyyy/MM/dd hh:mm:ss.fff}" />
            </Grid>
        </DataTemplate>
        <DataTemplate x:Key="AsyncOpenSessionTemplate" DataType="winBiometricDotNet:AsyncResult">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="5" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="5" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="5" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="5" />
                    <RowDefinition Height="Auto" />
