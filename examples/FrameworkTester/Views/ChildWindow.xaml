        <DataTemplate x:Key="ChildWindowLogTemplateSlectorEnrollCommit" DataType="winBiometricDotNet:AsyncResultEnrollCommit">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="5" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="5" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <TextBlock
                    Grid.Row="0"
                    Grid.Column="0"
                    VerticalAlignment="Top"
                    Style="{StaticResource LabelTextBlock}"
                    Text="Identity: " />
                <Grid
                    Grid.Row="0"
                    Grid.Column="2"
                    DataContext="{Binding Identity}">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="5" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="5" />
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="5" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <TextBlock
                        Grid.Row="0"
                        Grid.Column="0"
                        Style="{StaticResource LabelTextBlock}"
                        Text="Type: " />
                    <TextBlock
                        Grid.Row="0"
                        Grid.Column="2"
                        Style="{StaticResource ValueTextBlock}"
                        Text="{Binding Type}" />
                    <TextBlock
                        Grid.Row="2"
                        Grid.Column="0"
                        Style="{StaticResource LabelTextBlock}"
                        Text="TemplateGuid: " />
                    <TextBlock
                        Grid.Row="2"
                        Grid.Column="2"
                        Style="{StaticResource ValueTextBlock}"
                        Text="{Binding TemplateGuid}" />
                    <TextBlock
                        Grid.Row="4"
                        Grid.Column="0"
                        Style="{StaticResource LabelTextBlock}"
                        Text="Sid: " />
                    <TextBlock
                        Grid.Row="4"
                        Grid.Column="2"
                        Style="{StaticResource ValueTextBlock}"
                        Text="{Binding Sid}" />
                </Grid>
                <TextBlock
                    Grid.Row="2"
                    Grid.Column="0"
                    Style="{StaticResource LabelTextBlock}"
                    Text="IsNewTemplate: " />
                <TextBlock
                    Grid.Row="2"
                    Grid.Column="2"
                    Style="{StaticResource ValueTextBlock}"
                    Text="{Binding IsNewTemplate}" />
            </Grid>
        </DataTemplate>
        <DataTemplate x:Key="ChildWindowLogTemplateSlectorEnumBiometricUnits" DataType="winBiometricDotNet:AsyncResultEnumBiometricUnits">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="5" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <TextBlock
                    Grid.Row="0"
                    Grid.Column="0"
                    VerticalAlignment="Top"
                    Style="{StaticResource LabelTextBlock}"
                    Text="Units: " />
                <ScrollViewer
                    Grid.Row="0"
                    Grid.Column="2"
                    VerticalAlignment="Stretch">
                    <ListBox Height="300" ItemsSource="{Binding BiometricUnits}">
                        <ListBox.ItemTemplate>
                            <DataTemplate>
                                <Grid>
                                    <TextBlock Text="{Binding Description}" />
                                </Grid>
                            </DataTemplate>
                        </ListBox.ItemTemplate>
                    </ListBox>
                </ScrollViewer>
            </Grid>
        </DataTemplate>
        <DataTemplate x:Key="ChildWindowLogTemplateSlectorEnumDatabases" DataType="winBiometricDotNet:AsyncResultEnumDatabases">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="5" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <TextBlock
                    Grid.Row="0"
                    Grid.Column="0"
                    VerticalAlignment="Top"
                    Style="{StaticResource LabelTextBlock}"
                    Text="Databases: " />
                <ComboBox
                    Grid.Row="0"
                    Grid.Column="2"
                    VerticalAlignment="Stretch"
                    ItemsSource="{Binding Databases}">
                    <ComboBox.ItemTemplate>
                        <DataTemplate>
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="5" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.Column="0"
                                    Style="{StaticResource LabelTextBlock}"
                                    Text="DatabaseId: " />
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.Column="2"
                                    Style="{StaticResource ValueTextBlock}"
                                    Text="{Binding DatabaseId}" />
                                <TextBlock
                                    Grid.Row="1"
                                    Grid.Column="0"
                                    Style="{StaticResource LabelTextBlock}"
                                    Text="DataFormat: " />
                                <TextBlock
                                    Grid.Row="1"
                                    Grid.Column="2"
                                    Style="{StaticResource ValueTextBlock}"
                                    Text="{Binding DataFormat}" />
                                <TextBlock
                                    Grid.Row="2"
                                    Grid.Column="0"
                                    Style="{StaticResource LabelTextBlock}"
                                    Text="BiometricFactor: " />
                                <TextBlock
                                    Grid.Row="2"
                                    Grid.Column="2"
                                    Style="{StaticResource ValueTextBlock}"
                                    Text="{Binding BiometricFactor}" />
                                <TextBlock
                                    Grid.Row="3"
                                    Grid.Column="0"
                                    Style="{StaticResource LabelTextBlock}"
                                    Text="DatabaseFlag: " />
                                <TextBlock
                                    Grid.Row="3"
                                    Grid.Column="2"
                                    Style="{StaticResource ValueTextBlock}"
                                    Text="{Binding DatabaseFlag}" />
                                <TextBlock
                                    Grid.Row="4"
                                    Grid.Column="0"
                                    Style="{StaticResource LabelTextBlock}"
                                    Text="DatabaseTypes: " />
                                <TextBlock
                                    Grid.Row="4"
                                    Grid.Column="2"
                                    Style="{StaticResource ValueTextBlock}"
                                    Text="{Binding DatabaseTypes}" />
                                <TextBlock
                                    Grid.Row="5"
                                    Grid.Column="0"
                                    Style="{StaticResource LabelTextBlock}"
                                    Text="ConnectionString: " />
                                <TextBlock
                                    Grid.Row="5"
                                    Grid.Column="2"
                                    Style="{StaticResource ValueTextBlock}"
                                    Text="{Binding ConnectionString}" />
                                <TextBlock
                                    Grid.Row="6"
                                    Grid.Column="0"
                                    Style="{StaticResource LabelTextBlock}"
                                    Text="FilePath: " />
                                <TextBlock
                                    Grid.Row="6"
                                    Grid.Column="2"
                                    Style="{StaticResource ValueTextBlock}"
                                    Text="{Binding FilePath}" />
                            </Grid>
                        </DataTemplate>
                    </ComboBox.ItemTemplate>
                </ComboBox>
            </Grid>
        </DataTemplate>
        <DataTemplate x:Key="ChildWindowLogTemplateSlectorEnumEnrollments" DataType="viewModels:AsyncResultEnumEnrollmentsDummy">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="5" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <TextBlock
                    Grid.Row="0"
                    Grid.Column="0"
                    VerticalAlignment="Top"
