                                    Style="{StaticResource ValueTextBlock}"
                                    Text="{Binding Vendor}" />
                                <TextBlock
                                    Grid.Row="5"
                                    Grid.Column="0"
                                    Style="{StaticResource LabelTextBlock}"
                                    Text="Description: " />
                                <TextBlock
                                    Grid.Row="5"
                                    Grid.Column="2"
                                    Style="{StaticResource ValueTextBlock}"
                                    Text="{Binding Description}" />
                            </Grid>
                        </DataTemplate>
                    </ComboBox.ItemTemplate>
                </ComboBox>
            </Grid>
        </DataTemplate>
        <DataTemplate x:Key="ChildWindowLogTemplateSlectorIdentity" DataType="winBiometricDotNet:AsyncResultIdentity">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="5" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="5" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="5" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <TextBlock
                    Grid.Row="0"
                    Grid.Column="0"
                    VerticalAlignment="Top"
                    Style="{StaticResource LabelTextBlock}"
                    Text="Identity: " />
                <Grid
                    Grid.Row="0"
                    Grid.Column="2"
                    DataContext="{Binding Identity}">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="5" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="5" />
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="5" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <TextBlock
                        Grid.Row="0"
                        Grid.Column="0"
                        Style="{StaticResource LabelTextBlock}"
                        Text="Type: " />
                    <TextBlock
                        Grid.Row="0"
                        Grid.Column="2"
                        Style="{StaticResource ValueTextBlock}"
                        Text="{Binding Type}" />
                    <TextBlock
                        Grid.Row="2"
                        Grid.Column="0"
                        Style="{StaticResource LabelTextBlock}"
                        Text="TemplateGuid: " />
                    <TextBlock
                        Grid.Row="2"
                        Grid.Column="2"
                        Style="{StaticResource ValueTextBlock}"
                        Text="{Binding TemplateGuid}" />
                    <TextBlock
                        Grid.Row="4"
                        Grid.Column="0"
                        Style="{StaticResource LabelTextBlock}"
                        Text="Sid: " />
                    <TextBlock
                        Grid.Row="4"
                        Grid.Column="2"
                        Style="{StaticResource ValueTextBlock}"
                        Text="{Binding Sid}" />
                </Grid>
                <TextBlock
                    Grid.Row="2"
                    Grid.Column="0"
                    Style="{StaticResource LabelTextBlock}"
                    Text="RejectDetail: " />
                <TextBlock
                    Grid.Row="2"
                    Grid.Column="2"
                    Style="{StaticResource ValueTextBlock}"
                    Text="{Binding RejectDetail}" />
                <TextBlock
                    Grid.Row="4"
                    Grid.Column="0"
                    Style="{StaticResource LabelTextBlock}"
                    Text="FingerPosition: " />
                <TextBlock
                    Grid.Row="4"
                    Grid.Column="2"
                    Style="{StaticResource ValueTextBlock}"
                    Text="{Binding FingerPosition}" />
            </Grid>
        </DataTemplate>
        <DataTemplate x:Key="ChildWindowLogTemplateSlectorVerify" DataType="winBiometricDotNet:AsyncResultVerify">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="5" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="5" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <TextBlock
                    Grid.Row="0"
                    Grid.Column="0"
                    Style="{StaticResource LabelTextBlock}"
                    Text="Match: " />
                <TextBlock
                    Grid.Row="0"
                    Grid.Column="2"
                    Style="{StaticResource ValueTextBlock}"
                    Text="{Binding Match}" />
                <TextBlock
                    Grid.Row="2"
                    Grid.Column="0"
                    Style="{StaticResource LabelTextBlock}"
                    Text="RejectDetail: " />
                <TextBlock
                    Grid.Row="2"
                    Grid.Column="2"
                    Style="{StaticResource ValueTextBlock}"
                    Text="{Binding RejectDetail}" />
            </Grid>
        </DataTemplate>

        <selectors:ChildWindowLogTemplateSlector x:Key="ChildWindowLogTemplateSlector"
                                                 CaptureSample="{StaticResource ChildWindowLogTemplateSlectorCaptureSample}"
                                                 DeleteTemplate="{StaticResource ChildWindowLogTemplateSlectorDeleteTemplate}"
                                                 EnrollBegin="{StaticResource ChildWindowLogTemplateSlectorEnrollBegin}"
                                                 EnrollCapture="{StaticResource ChildWindowLogTemplateSlectorEnrollCapture}"
                                                 EnrollCommit="{StaticResource ChildWindowLogTemplateSlectorEnrollCommit}"
                                                 EnumBiometricUnits="{StaticResource ChildWindowLogTemplateSlectorEnumBiometricUnits}"
                                                 EnumDatabases="{StaticResource ChildWindowLogTemplateSlectorEnumDatabases}"
                                                 EnumEnrollments="{StaticResource ChildWindowLogTemplateSlectorEnumEnrollments}"
                                                 EnumServiceProviders="{StaticResource ChildWindowLogTemplateSlectorEnumServiceProviders}"
                                                 Identity="{StaticResource ChildWindowLogTemplateSlectorIdentity}"
                                                 Verify="{StaticResource ChildWindowLogTemplateSlectorVerify}" />
    </Window.Resources>
    <Grid Margin="10">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
            <RowDefinition Height="200" />
        </Grid.RowDefinitions>

        <controls:ObservableContentControl
            Grid.Row="0"
            Content="{Binding SelectedAsyncResult}"
            ContentTemplateSelector="{StaticResource ChildWindowTemplateSlector}" />

        <controls:ObservableContentControl
            Grid.Row="1"
            Margin="0,15,0,15"
            Content="{Binding SelectedAsyncResultParameter}"
            ContentTemplateSelector="{StaticResource ChildWindowLogTemplateSlector}" />

        <Grid Grid.Row="2">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>

            <StackPanel
                Grid.Row="0"
                HorizontalAlignment="Right"
                Orientation="Horizontal">
                <Button
                    Margin="0,5,5,5"
                    HorizontalAlignment="Right"
                    Command="{Binding ClearLogsCommand}"
                    Content="Clear Logs" />
            </StackPanel>

            <ScrollViewer Grid.Row="1">
                <ListBox
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Stretch"
                    ItemsSource="{Binding AsyncResultLogs}"
                    SelectedItem="{Binding SelectedAsyncResult}">
                    <ListBox.ItemTemplate>
                        <DataTemplate>
                            <Grid>
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Style="{StaticResource ValueTextBlock}" Text="[" />
                                    <TextBlock Style="{StaticResource ValueTextBlock}" Text="{Binding Path=TimeStamp, StringFormat=yyyy/MM/dd hh:mm:ss.fff}" />
                                    <TextBlock Style="{StaticResource ValueTextBlock}" Text="] [" />
                                    <TextBlock Style="{StaticResource ValueTextBlock}" Text="{Binding OperationType}" />
                                    <TextBlock Style="{StaticResource ValueTextBlock}" Text="]" />
                                </StackPanel>
                            </Grid>
                        </DataTemplate>
                    </ListBox.ItemTemplate>
                </ListBox>
            </ScrollViewer>
        </Grid>
    </Grid>
</Window>
