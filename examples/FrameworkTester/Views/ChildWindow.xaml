                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <TextBlock
                    Grid.Row="0"
                    Grid.Column="0"
                    Style="{StaticResource LabelTextBlock}"
                    Text="ApiStatus: " />
                <TextBlock
                    Grid.Row="0"
                    Grid.Column="2"
                    Style="{StaticResource ValueTextBlock}"
                    Text="{Binding ApiStatus}" />
                <TextBlock
                    Grid.Row="2"
                    Grid.Column="0"
                    Style="{StaticResource LabelTextBlock}"
                    Text="OperationType: " />
                <TextBlock
                    Grid.Row="2"
                    Grid.Column="2"
                    Style="{StaticResource ValueTextBlock}"
                    Text="{Binding OperationType}" />
                <TextBlock
                    Grid.Row="4"
                    Grid.Column="0"
                    Style="{StaticResource LabelTextBlock}"
                    Text="SequenceNumber: " />
                <TextBlock
                    Grid.Row="4"
                    Grid.Column="2"
                    Style="{StaticResource ValueTextBlock}"
                    Text="{Binding SequenceNumber}" />
                <TextBlock
                    Grid.Row="6"
                    Grid.Column="0"
                    Style="{StaticResource LabelTextBlock}"
                    Text="Session Handle: " />
                <TextBlock
                    Grid.Row="6"
                    Grid.Column="2"
